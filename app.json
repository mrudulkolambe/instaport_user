{
  "expo": {
    "name": "InstaPort Delivery",
    "slug": "instaport-delivery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow InstaPort Delivery to use your location.",
          "isAndroidBackgroundLocationEnabled": true
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCQb159dbqJypdIO1a1o0v_mNgM5eFqVAo"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mrudulkolambe.instaportdelivery",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCQb159dbqJypdIO1a1o0v_mNgM5eFqVAo"
        }
      }
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "7e320b11-b40f-4196-8609-2675b29f2200"
      }
    },
    "owner": "mrudulkolambe"
  }
